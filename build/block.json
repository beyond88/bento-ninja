{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/bento-ninja",
  "version": "0.1.0",
  "title": "Bento Ninja",
  "category": "layout",
  "icon": "grid-view",
  "description": "Bento Ninja is a stunning block to create bento layout easily.",
  "attributes": {
    "align": {
      "type": "string",
      "default": "none"
    },
    "gridColumns": {
      "type": "number",
      "default": 4
    }
  },
  "supports": {
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "color": {
      "gradients": true,
      "heading": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "bento-ninja",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}